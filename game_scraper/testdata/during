		(function($){
		//checking insider access for insider module on preview page
	    	$(document).ready(function(){
		    	var insiderAccess = false;
			    if(espn.core.loggedIn == true){
			        szprem = espn.storage.getItem('szpremToken');
			        insiderDirect = espn.storage.getItem('insiderDirectToken');
			        if(szprem == true || insiderDirect == true){
			            insiderAccess = true;
			        }
			    }
			    if(insiderAccess){
			    	$("div#insiderTrue").show();
			    }
			    if(!insiderAccess){
			    	$("div#insiderFalse").show();
			    	$("div#insiderTrue").html('');
			    }
			});
    	}(jQuery));
